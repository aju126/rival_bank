<p id="notice"><%= notice %></p>

<h1>Listing Account Interests</h1>

<table>
  <thead>
    <tr>
      <th>Interest</th>
      <th>Account name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_interests.each do |account_interest| %>
      <tr>
        <td><%= account_interest.interest %></td>
        <td><%= account_interest.account_name %></td>
        <td><%= link_to 'Show', account_interest %></td>
        <td><%= link_to 'Edit', edit_account_interest_path(account_interest) %></td>
        <td><%= link_to 'Destroy', account_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account interest', new_account_interest_path %>
